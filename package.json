{
  "name": "boilerplate-nextjs-antd-tailwindcss",
  "version": "0.1.0",
  "description": "A boilerplate for Reactjs app using nextjs, redux, redux-thunk, antd, tailwindcss",
  "repository": {
    "type": "git",
    "url": "git@github.com:tientran0019/boilerplate-nextjs-antd-tailwindcss.git"
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "antd",
    "tailwindcss"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "env-cmd next dev",
    "export": "env-cmd -f .env.production --fallback --silent next export",
    "build": "env-cmd -f .env.production --fallback --silent next build",
    "build:staging": "env-cmd -f .env.staging --fallback --silent next build",
    "start": "env-cmd -f .env.production --fallback --silent next start",
    "start:staging": "env-cmd -f .env.staging --fallback --silent next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.16.2",
    "@ant-design/icons": "^5.2.5",
    "@ant-design/static-style-extract": "^1.0.1",
    "antd": "^5.8.4",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "plyr-react": "^5.3.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.15",
    "env-cmd": "^10.1.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3"
  }
}
